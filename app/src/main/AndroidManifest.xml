<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tour_guide">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cactus"
        android:label="Tucson Tour Guide"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".restaurants"
                  />
        <activity android:name=".bars"
                  />
        <activity android:name=".attractions"
                  />
        <activity android:name=".shopping"
                  />

    </application>

</manifest>